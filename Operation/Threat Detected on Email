blocks:
- type: sql
  query: SELECT $Threat, $Sender, $Recipient FROM THREAT WHERE $Action='THREAT_DETETCED'
    AND $Vector='EMAIL' GROUP BY $Threat, $Sender, $Recipient
- type: signal
  DetectionName: _$Threat_
  DetectionTactic: Initial Access
  DetectionTechnique: 'Phishing '
  DetectionScore: '5'
  DetectionSeverity: Medium
  DetectionConfidence: Medium
  SuspectUser: $Sender
  TargetUser: $Recipient
