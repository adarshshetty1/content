blocks:
- type: sql
  query: SELECT $ProcessName,$System  FROM AUDITD WHERE $Action='PROCESS_ADDED' AND $ProcessName='whoami' GROUP BY $ProcessName,$System 
- type: signal
  DetectionName: User Discovery via Whoami
  DetectionTactic: Discovery
  DetectionTechnique: System Owner/User Discovery
  TargetHost: $System
  DetectionScore: 5
  DetectionConfidence: Low