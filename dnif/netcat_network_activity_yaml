blocks:
- type: sql
  query: SELECT $ProcessName,$System  FROM AUDITD WHERE $Action='PROCESS_ADDED' AND ($ProcessName='nc' OR $ProcessName='ncat' OR $ProcessName='netcat' OR $ProcessName='netcat.openbsd' OR $ProcessName='netcat.traditional') GROUP BY $ProcessName,$System 
- type: signal
  DetectionName: Netcat Network Activity
  DetectionTactic: Discovery
  DetectionTechnique: System Network Connections Discovery
  TargetHost: $System
  DetectionScore: 5
  DetectionConfidence: Low